{
  "generateComponent": {
    "basePathForComponents": "./app/components/",
    "templatesPath": "./shell-utilities/generate-component/templates/",
    "minLengthOfComponentName": 2,
    "maxLengthOfComponentName": 31,
    "commonContentInGeneratedFiles": [
      {
        "key": "autoGeneratedComponentMessage",
        "value": "This code was automatically generated by Crestron's code generation tool."
      }
    ]
  },
  "export": {
    "templatesPath": "./shell-utilities/export/templates/",
    "zipFileDestinationPath": "./dist/",
    "ignoreFilesFolders": [
      "node_modules",
      "dist",
      "www",
      "/out-tsc",
      ".project",
      ".classpath",
      ".settings/",
      ".sublime-workspace",
      ".sass-cache",
      "npm-debug.log",
      "yarn-error.log",
      "testem.log",
      ".DS_Store",
      ".lock",
      "Thumbs.db"
    ]
  },
  "logger": {
    "allowLogging": false,
    "logLevel": 1
  }
}